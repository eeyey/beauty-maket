{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@components/": ["./components"],
      "@components/*": ["./components/*"],
      "@core/": ["./core"],
      "@core/*": ["./core/*"]
    }
  }
}
